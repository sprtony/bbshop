{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "althinect/filament-spatie-roles-permissions": "^2.0",
        "archilex/filament-toggle-icon-column": "^3.0",
        "aymanalhattami/filament-slim-scrollbar": "^2.0",
        "beyondcode/laravel-websockets": "^1.14",
        "buglinjo/laravel-webp": "^2.3",
        "calebporzio/sushi": "^2.5",
        "codeat3/blade-vaadin-icons": "^1.4",
        "codewithdennis/filament-select-tree": "^3.1",
        "eightynine/filament-excel-import": "^3.0",
        "filament/filament": "^3.0-stable",
        "filament/spatie-laravel-settings-plugin": "^3.0-stable",
        "filament/spatie-laravel-translatable-plugin": "^3.0-stable",
        "guava/filament-icon-picker": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "joshembling/image-optimizer": "^1.2",
        "laravel/folio": "^1.1",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^3.0",
        "livewire/volt": "^1.1",
        "mehradsadeghi/laravel-filter-querystring": "^1.1",
        "mohamedsabil83/filament-forms-tinyeditor": "^2.0",
        "owenvoke/blade-fontawesome": "^2.4",
        "pusher/pusher-php-server": "^7.2",
        "pxlrbt/filament-excel": "^2.1",
        "rawilk/filament-password-input": "^1.1",
        "solution-forest/filament-tree": "^2.0",
        "tightenco/ziggy": "^1.8"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "BlackBox\\Admin\\": "packages/BlackBox/Admin/src",
            "BlackBox\\Customers\\": "packages/BlackBox/Customers/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan ide-helper:generate"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
